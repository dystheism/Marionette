{
  "name": "marionette",
  "version": "0.0.0",
  "private": true,
  "main": "app.html",
  "window": {
    "width": 500,
    "height": 500,
    "show": true,
    "frame": true,
    "toolbar": true,
    "transparent": false,
    "show_in_taskbar": true
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dist": "build --tasks win-x64,mac-x64 --mirror https://dl.nwjs.io/ ."
  },
  "author": "Aliz√©e Wickenheiser",
  "license": "",
  "build": {
    "appId": "intralizee.Marionette",
    "nwVersion": "0.30.1",
    "packed": true,
    "targets": [
      "zip",
      "nsis7z"
    ],
    "strippedProperties": [
      "build"
    ],
    "outputPattern": "${NAME} ${VERSION} ${PLATFORM} ${ARCH}",
    "win": {
      "productName": "Marionette",
      "companyName": "intralizee",
      "copyright": "copyright",
      "icon": "./images/marionette/icon/icon.ico"
    },
    "mac": {
      "displayName": "Marionette",
      "copyright": "copyright",
      "icon": "./images/marionette/icon/icon.icns",
      "plistStrings": {
        "CFBundleIdentifier": "intralizee.Marionette",
        "CFBundleDocumentTypes": []
      }
    },
    "nsis": {
      "installDirectory": "$PROGRAMFILES\\${_COMPANYNAME}\\${_APPNAME}",
      "diffUpdaters": true
    }
  },
  "dependencies": {
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "json-loader": "^0.5.7",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.1",
    "webpack-cli": "^3.2.3"
  }
}
